  "keywords": [
    "cue",
    "cue-sheet",
    "parser",
    "audio",
    "cd",
    "typescript",
    "cli"
  ],
  "files": [
    "dist/",
    "bin/"
  ],
  "devDependencies": {
    "@types/node": "^20.19.24",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0",
    "@vitest/ui": "^2.0.0"
  }
}
