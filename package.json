{
  "name": "cue-parser",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "cue-parser": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "dev": "tsc --watch",
    "cli": "node bin/index.js"
  },
  "author": "MaxMEllon",
  "license": "MIT",
